{
    "manifest_version":3,
    "name":"A Desk Notify",
    "version":"1.0",
    "description":"A Desk Notify",
    "author":"Fatosy",
    "icons":{
        "16":"img/icon16.png",
        "48":"img/icon48.png",
        "128":"img/icon128.png"
    },
    "action":{
        "default_icon":"img/icon16.png",
        "default_title":"A Desk Notify",
        "default_popup":"popup.html"
    },
    "background":{
        "service_worker": "back.js",
        "type":"module"
    },
    "permissions":[
        "notifications",
        "tabs"
    ]
}