{
    "manifest_version":3,
    "name":"Browser Search",
    "version":"1.0",
    "description":"Browser Search",
    "author":"Fatosy",
    "icons":{
        "16":"img/icon16.png",
        "48":"img/icon48.png",
        "128":"img/icon128.png"
    },
    "action":{
        "default_icon":"img/icon16.png",
        "default_title":"Browser Search",
        "default_popup":"popup.html"
    },
    "background":{
        "service_worker":"js/back.js",
        "type":"module"
    },
    "permissions":[
        "contextMenus",
        "tabs"
    ],
    "omnibox":{
        "keyword":"baidu"
    }
}