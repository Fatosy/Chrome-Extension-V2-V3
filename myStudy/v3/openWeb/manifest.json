{
    "manifest_version":3,
    "name":"Open Web",
    "version":"1.0",
    "description":"Open Web",
    "author":"Fatosy",
    "icons":{
        "16":"img/icon16.png",
        "48":"img/icon48.png",
        "128":"img/icon128.png"
    },
    "action":{
        "default_icon":"img/icon16.png",
        "default_title":"Open Web",
        "default_popup":"popup.html"
    },
    "background":{
        "service_worker":"js/back.js",
        "type": "module"
    },
    "permissions":[
        "contextMenus",
        "tabs"
    ],
    "chrome_url_overrides":{
        "newtab":"new.html"
    }
}