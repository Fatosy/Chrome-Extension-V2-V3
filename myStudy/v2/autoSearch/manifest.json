{
    "manifest_version":2,
    "name":"autoSearch",
    "version":"1.0",
    "description":"autoSearch for baidu",
    "author":"Fatosy",
    "icons":{
        "16":"img/icon16.png",
        "48":"img/icon48.png",
        "128":"img/icon128.png"
    },
    "browser_action":{
        "default_icon":"img/icon16.png",
        "default_title":"A autoSearch sofware",
        "default_popup":"popup.html"
    },
    "background":{
        "page":"back.html"
    },
    "permissions":[
        "tabs",
        "*://*/*",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["baidu.js"],
            "run_at":"document_start"
        }
    ]
}