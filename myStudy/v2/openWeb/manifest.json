{
    "manifest_version":2,
    "name":"Open Web",
    "version":"1.0",
    "description":"Open Web",
    "author":"Fatosy",
    "icons":{
        "16":"img/icon16.png",
        "48":"img/icon48.png",
        "128":"img/icon128.png"
    },
    "browser_action":{
        "default_icon":"img/icon16.png",
        "default_title":"Open Web",
        "default_popup":"popup.html"
    },
    "background":{
        "page":"back.html"
    },
    "permissions":[
        "contextMenus",
        "tabs",
        "*://*/*",
        "http://*/*",
        "https://*/*"
    ],
    "chrome_url_overrides":{
        "newtab":"new.html"
    }
}